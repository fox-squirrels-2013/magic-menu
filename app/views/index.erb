<div class="container">
  <form id="menu_form" action="/menu/new" method="post">
    <input id="menu_name" type="text" name="menu[name]" placeholder="Menu Name">
    <input id="menu_submit" type="submit" value="submit">
  </form>
  <div id="menu_list">
    <% @menus.each do |menu| %>
      <p><a href="menu/<%= menu.id %>"><%= menu.name %></a><a class="delete_menu" id="<%= menu.id %>" href="#"> Delete </a></p>
    <% end %>
  </div>
</div>
