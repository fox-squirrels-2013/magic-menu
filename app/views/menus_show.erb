<div class="partial-container">

  <h2>the menus you created...</h2>

  <div id="menu_list">

    <% @menus.each do |menu| %>
      <p class="menu-to-delete">
        <form action="/menus/delete/<%= menu.id %>" method="post">
          <input type="submit" class="button-d" value="X">
            <a href="/menus/update/<%= menu.id %>" class="menu-link"><%= menu.name %></a>
        </form>
      </p>
    <% end %>

  </div>
</div>